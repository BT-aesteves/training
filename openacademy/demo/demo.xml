<odoo>
    <data>
        <record model="openacademy.course" id="course_1">
            <field name="name">Course Functional Training</field>
            <field name="description">Introduction to Odoo from the
                functional perspective.</field>
        </record>
        <record model="openacademy.course" id="course_2">
            <field name="name">Course Technical Training</field>
            <field name="description">Introduction to Odoo from the
                technical perspective.</field>
        </record>
        <record model="res.partner.category" id="category_teacher">
            <field name="name">Teacher</field>
        </record>
        <record model="res.partner.category" id="category_teacher_1">
            <field name="name">Level 1</field>
            <field name="parent_id" ref="category_teacher"/>
        </record>
        <record model="res.partner.category" id="category_teacher_2">
            <field name="name">Level 2</field>
            <field name="parent_id" ref="category_teacher"/>
        </record>
    </data>
</odoo>