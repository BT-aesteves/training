<odoo>
    <data>
        <record model="openacademy.course" id="course_1">
            <field name="name">Course Functional Training</field>
            <field name="description">Introduction to Odoo from the
                functional perspective.</field>
        </record>
        <record model="openacademy.course" id="course_2">
            <field name="name">Course Technical Training</field>
            <field name="description">Introduction to Odoo from the
                technical perspective.</field>
        </record>
