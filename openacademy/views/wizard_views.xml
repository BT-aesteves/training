<?xml version="1.0"?>
<odoo>

    <record id="wizard_form_view" model="ir.ui.view">
        <field name="name">FORM VIEW</field>
        <field name="model">wizard</field>
        <field name="arch" type="xml">
            <form string="form_view">
                <group>
                    <field name="sessions" nolabel="1"/>
                </group>
                <group>
                    <notebook>
                       <page string="Attendees">
                           <field name="attendees" nolabel="0"/>
                       </page>
                   </notebook>
                </group>
                <footer>
                    <button name="add_attendees" string="add_attendees" type="object"/>
                    <button string="Cancel" class="btn-default" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <act_window id="WIZARD"
            name="Session and Attendees"
            src_model="sessions"
            res_model="wizard"
            view_mode="form"
            target="new"
            context="{'default_session': active_id}"
            key2="client_action_multi"/>

</odoo>