<odoo>
    <data>

        <record model="ir.ui.view" id="session_wizard_form_view">
            <field name="name">Add attendees to session</field>
            <field name="model">add_sessions_wizard</field>
            <field name="arch" type="xml">
                <form string="Attendees">
                    <group colspan="4">
                        <group colspan="2">
                            <!--<field name="wizard_session"/>-->
                        </group>
                        <notebook colspan="4">
                            <page string="List of Attendees">
                                <field name="wizard_session_attendees"/>
                            </page>
                        </notebook>
                    </group>
                    <footer>
                        <button string="Save" type="object" name="save_results"/>
                        <button string="Cancel" type="object" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <act_window id="session_wizard"
                    name="Add attendees to the session"
                    src_model="session"
                    res_model="add_sessions_wizard"
                    view_mode="form"
                    target="new"
                    key2="client_action_multi"
                    context="{'default_wizard_session': active_id}"
        />
    </data>
</odoo>