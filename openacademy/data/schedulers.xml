<odoo>
    <data noupdate="1">
        <record id="confirmed2done_cron" model="ir.cron">
            <field name="name">Sessions past: Confirmed to Done </field>
            <field name="active" eval="True"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="model_id" ref="openacademy.model_session" />
            <field name="state">code</field>
            <field name="code">model.cron_confirmed2done()</field>
        </record>
   </data>
</odoo>
