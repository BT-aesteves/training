<odoo>
    <template id="report_session">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="session">
                <t t-call="web.external_layout">
                    <div class="page">
                        <h1 t-field="session.name"></h1>
                        <h3>Dates</h3>
                        <div t-field="session.start_date"></div>
                        <div t-field="session.end_date"></div>
                        <h3>Attendees</h3>
                        <t t-foreach="session.attend_ids" t-as="attendee">
                            <li><span t-field="attendee.name" /></li>
                        </t>
                    </div>
                </t>
            </t>
        </t>
    </template>

    <report id="openacademy.sessions"
            model="openacademy.session"
            string="Sessions details"
            report_type="qweb-pdf"
            name="openacademy.report_session"
            file="openacademy.report_session"
    />
</odoo>