<odoo>
    <data>
        <template id="report_session_template">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="r">
                    <t t-call="web.external_layout">
                        <div class="page">
                            <h1 t-field="r.name" />
                            <p>From <span t-field="r.start_date"/> to <span t-field="r.end_date"/></p>
                            <h3>Attendees:</h3>
                            <ul>
                            <t t-foreach="r.attendees" t-as="a">
                                <li><span t-field="a.name" /></li>
                            </t>
                            </ul>
                        </div>
                    </t>
                </t>
            </t>
        </template>


        <report id="report_session_id"
                model="session"
                string="Session Report"
                name="openacademy.report_session_template"
                file="openacademy.report_session_template"
                report_type="qweb-pdf"
                attachment_use="False"
                attachment="('report.pdf')"
        />
    </data>
</odoo>