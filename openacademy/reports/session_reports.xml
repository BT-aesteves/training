<odoo>
    <data>
        <template id="session_report_template">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-call="web.external_layout">
                        <div class="page">
                            <h1 t-field="o.name"/>
                            <h1 t-field="o.start_date"/>
                            <h1 t-field="o.end_date"/>
                            <t t-foreach="o.attendees" t-as="attendee">
                                <li>
                                    <span t-field="attendee.name"/>
                                </li>
                            </t>
                        </div>
                    </t>
                </t>
            </t>
        </template>

        <report id="session_report_id"
                model="session"
                string="Sessions Report"
                report_type="qweb-pdf"
                name="openacademy.session_report_template"
                file="openacademy.session_report_template"
                attachment_use="False"
                attachment="('sessionReport'+(object.name or '').replace('/','')+'.pdf')"/>

    </data>
</odoo>