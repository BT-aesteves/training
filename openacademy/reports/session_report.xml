<odoo>
    <data>
        <template id="report_session">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-call="web.external_layout">
                        <div class="page">
                            <h1 t-field="o.name"/>
                            <hr/>
                            <h2 t-field="o.start_date"/>
                            <h2 t-field="o.end_date"/>
                            <div>
                                <t t-foreach="o.attendees" t-as="attendee">
                                    <span t-field="attendee.name"/><br />
                                </t>
                            </div>
                        </div>
                    </t>
                </t>
            </t>
        </template>

        <report id="session_attendance"
                model="openacademy.session"
                string="Session attendance"
                report_type="qweb-pdf"
                name="openacademy.report_session"
                file="openacademy.report_session"
                attachment_use="False"
                attachment="('SESSION'+str(object.id or '')+'.pdf')"/>
    </data>
</odoo>