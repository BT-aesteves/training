<odoo>
    <template id="session_report">
          <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="web.external_layout">
                    <div class="page">
                        <h1 t-field="o.name"/>
                        <p t-field="o.start_date"/>
                        <p t-field="o.end_date"/>
                        <t t-foreach="o.attendees" t-as="attendee">
                           <li><span t-field="attendee.name" /></li>
                        </t>
                    </div>
                </t>
            </t>
          </t>
    </template>

    <report id="session_report"
        model="session"
        string="Session Report"
        report_type="qweb-pdf"
        name="openacademy.session_report"
        file="openacademy.session_report"
        attachment_use="False"
        attachment="('Session_'+(object.name or '')+'.pdf')" />
</odoo>