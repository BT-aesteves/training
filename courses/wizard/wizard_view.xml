<odoo>
    <act_window id="wizard_view_action"
            name="register.attendees.action"
            binding_model="session"
            res_model="wizard"
            view_mode="form" target="new"/>

    <record id="wizard_view" model="ir.ui.view">
        <field name="name">register.attendees.view</field>
        <field name="model">wizard</field>
        <field name="arch" type="xml">
            <form string="Register Attendees">
                <group>
                    <field name="session_id"/>
                </group>
                <notebook colspan="4">
                  <page string="Attendees">
                      <field name="attendees" nolabel="1"></field>
                  </page>
                </notebook>
                <footer>
                    <button name="register_attendees" type="object" string="Register" class="btn-primary"/>
                    <button string="Cancel" special="cancel" class="btn-secondary"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>